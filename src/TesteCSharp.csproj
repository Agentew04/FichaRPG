<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>TesteCSharp.App</StartupObject>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>keys.pfx</AssemblyOriginatorKeyFile>
    <PackageId>Ficha de RPG</PackageId>
    <Authors>Rodrigo Appelt</Authors>
    <Company>Rodrigo's Stuff</Company>
    <Product>Ficha de RPG</Product>
    <Description>Um aplicativo criado para auxiliar jogares de RPG de mesa online.</Description>
    <AssemblyName>FichaRPG</AssemblyName>
    <NeutralLanguage>pt-BR</NeutralLanguage>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="PgpCore" Version="5.1.0" />
  </ItemGroup>

</Project>
